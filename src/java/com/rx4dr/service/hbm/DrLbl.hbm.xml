<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rx4dr.service.model.DrLbl" table="dr_lbl" catalog="rx4dr">
        <id name="IDocIbl" type="int">
            <column name="I_DOC_IBL" />
            <generator class="assigned" />
        </id>
        <property name="IDrId" type="int">
            <column name="I_DR_ID" not-null="true" />
        </property>
        <property name="lbl1" type="string">
            <column name="LBL_1" length="60" not-null="true" />
        </property>
        <property name="lbl2" type="string">
            <column name="LBL_2" length="45" />
        </property>
        <property name="lbl3" type="string">
            <column name="LBL_3" length="45" />
        </property>
        <property name="lbl4" type="string">
            <column name="LBL_4" length="45" />
        </property>
    </class>
</hibernate-mapping>
